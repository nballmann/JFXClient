<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="450.0" prefWidth="450.0" xmlns:fx="http://javafx.com/fxml" fx:controller="org.nic.jfxclient.controller.MainViewController">
  <!-- TODO Add Nodes -->
  <children>
    <BorderPane prefHeight="200.0" prefWidth="200.0">
      <center>
        <Pane prefHeight="200.0" prefWidth="200.0" styleClass="background">
          <children>
            <TextArea id="queryTextArea" fx:id="queryTextArea" editable="true" layoutX="230.0" layoutY="14.0" prefHeight="396.0" prefWidth="206.0" wrapText="true" />
            <VBox id="VBox" alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="396.0" prefWidth="206.0" spacing="5.0">
              <children>
                <StackPane prefHeight="396.0" prefWidth="206.0" />
              </children>
            </VBox>
          </children>
        </Pane>
      </center>
      <top>
        <MenuBar>
          <menus>
            <Menu mnemonicParsing="false" text="File">
              <items>
                <MenuItem mnemonicParsing="false" text="Close" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
              <items>
                <MenuItem mnemonicParsing="false" text="Delete" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Help">
              <items>
                <MenuItem mnemonicParsing="false" text="About" />
              </items>
            </Menu>
          </menus>
        </MenuBar>
      </top>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@main_layout.css" />
  </stylesheets>
</StackPane>
